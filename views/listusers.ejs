<!doctype html>
<html>
  <head>
    <title><%= shopData.shopName %></title>
    <link rel="stylesheet" type="text/css" href="/main.css" />
  </head>
  <body>
    <h1>User List for <%= shopData.shopName %></h1>
    <h2>Registered Users:</h2>
    <table border="1">
      <tr>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>
      <% userList.forEach(function(user) { %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.email %></td>
        </tr>
      <% }) %>
    </table>
  </body>
</html>
